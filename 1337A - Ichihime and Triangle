#include<bits/stdc++.h>
using namespace std;
int main(){
    int t,a,b,c,d,x=0,y=0,z=0;
    cin>>t;
    while(t--){
        cin>>a>>b>>c>>d;
        if(a>b)
            x=a;
        else
            x=b;
        if(b>c)
            y=b;
        else
            y=c;
        if(c<d)
            z=c;
        else
            z=d;
            cout<<x<<" "<<y<<" "<<z<<endl;
                }
    return 0;
}
