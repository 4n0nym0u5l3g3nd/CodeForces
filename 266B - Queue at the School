#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,x,i,j;
    string s;
    cin>>n>>x;
    cin>>s;
    for(i=0;i<x;i++){
        for(j=0;j<n;j++){
            if(s[j]=='B'&&s[j+1]=='G'){
                swap(s[j],s[j+1]);
            j++;
            }
        }
    }
    cout<<s<<endl;
    return 0;
}
