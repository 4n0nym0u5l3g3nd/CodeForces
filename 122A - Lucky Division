#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,a[]={4,7,47,74,477,474,447,744,747,774},i,c=0;
    cin>>n;
    for(i=0;i<10;i++){
        if(n==a[i] || n%a[i]==0){
            c++;
            break;}
        else
          c=0;

    }
    if(c!=0)
    cout<<"YES"<<endl;
    else
    cout<<"NO"<<endl;
    return 0;
}
