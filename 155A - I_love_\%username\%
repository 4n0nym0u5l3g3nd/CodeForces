#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,a,i,s=0,mx=0,mn=0;
    cin>>n>>a;
    mx=a;
    mn=a;
    for(i=1;i<n;i++){
        cin>>a;
        if(a<mn){
            s++;
            mn=a;
        }
        if(a>mx){
            s++;
            mx=a;
        }
        }
    cout<<s<<endl;
    return 0;
}
