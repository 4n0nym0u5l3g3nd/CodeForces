#include<bits/stdc++.h>
using namespace std;
int main(){
    string a,b;
    int i,j,l1,l2;
    cin>>a>>b;
    l1=a.length();
    l2=b.length();
    for(i=0;i<l1;i++){
        if(a[i]<92)
            a[i]+=32;
    }
    for(i=0;i<l2;i++){
        if(b[i]<92)
            b[i]+=32;
    }
    if(a==b)
        cout<<"0"<<endl;
    else if(a>b)
        cout<<"1"<<endl;
    else
        cout<<"-1"<<endl;
    return 0;
}

